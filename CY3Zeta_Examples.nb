Cell["\<\
To demonstrate all different functions, we will have to refresh the functions \
and constant values saved in memory multiple times. To make this easier, we \
define an auxiliary function that sets up these same initial values, apart \
from the NMax, which we will vary, depending on what is needed for particular \
computations.\
\>", "Text",
 CellChangeTimes->{{3.904121565394755*^9, 
  3.904121718501215*^9}},ExpressionUUID->"3a6459e9-ecd1-4804-9d88-\
eb5af3b819bb"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"init", "[", "NMax_", "]"}], ":=", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"zSetNParameters", "[", "2", "]"}], ";", 
     RowBox[{"zSetNMax", "[", "NMax", "]"}], ";", 
     RowBox[{"zSetY", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"Y", "[", 
          RowBox[{"1", ",", "1", ",", "1"}], "]"}], "->", "0"}], ",", 
        RowBox[{
         RowBox[{"Y", "[", 
          RowBox[{"1", ",", "1", ",", "2"}], "]"}], "->", "0"}], ",", 
        RowBox[{
         RowBox[{"Y", "[", 
          RowBox[{"2", ",", "1", ",", "2"}], "]"}], "->", "4"}], ",", 
        RowBox[{
         RowBox[{"Y", "[", 
          RowBox[{"2", ",", "2", ",", "2"}], "]"}], "->", "5"}]}], "}"}], 
      "]"}], ";", 
     RowBox[{"zSetYhat", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"Yhat", "[", 
          RowBox[{"1", ",", "2", ",", "1"}], "]"}], "\[Rule]", 
         FractionBox[
          RowBox[{"-", "5"}], "32"]}], ",", 
        RowBox[{
         RowBox[{"Yhat", "[", 
          RowBox[{"1", ",", "2", ",", "2"}], "]"}], "\[Rule]", 
         FractionBox["1", "8"]}], ",", 
        RowBox[{
         RowBox[{"Yhat", "[", 
          RowBox[{"2", ",", "2", ",", "1"}], "]"}], "\[Rule]", 
         FractionBox["1", "4"]}], ",", 
        RowBox[{
         RowBox[{"Yhat", "[", 
          RowBox[{"2", ",", "2", ",", "2"}], "]"}], "\[Rule]", "0"}], ",", 
        RowBox[{
         RowBox[{"Yhat", "[", 
          RowBox[{"1", ",", "1", ",", "1"}], "]"}], "\[Rule]", "0"}], ",", 
        RowBox[{
         RowBox[{"Yhat", "[", 
          RowBox[{"1", ",", "1", ",", "2"}], "]"}], "\[Rule]", "0"}]}], "}"}],
       "]"}], ";", 
     RowBox[{"zSetConifoldLocus", "[", 
      RowBox[{
       RowBox[{"-", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           RowBox[{"-", "1"}], "+", 
           RowBox[{"\[Phi]", "[", "1", "]"}]}], ")"}], "5"]}], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "512"}], "+", 
          RowBox[{
           RowBox[{"\[Phi]", "[", "1", "]"}], " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "2816"}], "+", 
             RowBox[{
              RowBox[{"\[Phi]", "[", "1", "]"}], " ", 
              RowBox[{"(", 
               RowBox[{"320", "+", 
                RowBox[{"9", " ", 
                 RowBox[{"\[Phi]", "[", "1", "]"}], " ", 
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"-", "16"}], "+", 
                   RowBox[{"3", " ", 
                    RowBox[{"\[Phi]", "[", "1", "]"}]}]}], ")"}]}]}], 
               ")"}]}]}], ")"}]}]}], ")"}], " ", 
        RowBox[{"\[Phi]", "[", "2", "]"}]}], "+", 
       RowBox[{"65536", " ", 
        SuperscriptBox[
         RowBox[{"\[Phi]", "[", "2", "]"}], "2"]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"zSetOtherSingularLocus", "[", "1", "]"}], ";", 
     RowBox[{"zSetDirectory", "[", "\"\<Split_Quintic\>\"", "]"}], ";"}], 
    ")"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.9041214211985273`*^9, 3.904121466361683*^9}, {
   3.9041296332061453`*^9, 3.904129638098229*^9}, 3.904205036922955*^9},
 CellLabel->"In[8]:=",ExpressionUUID->"311713a2-37f1-464f-bbf4-6b557b4ff6c0"]

Cell[CellGroupData[{

Cell[TextData[{
 "Finding the coefficients ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Alpha]", "i"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "85a675b4-17f3-48a7-8971-268230e0ec76"],
 " and \[Gamma]"
}], "Section",

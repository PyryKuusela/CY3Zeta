Cell[TextData[{
 "Once the matrices ",
 Cell[BoxData[
  FormBox[
   RowBox[{"E", "(", 
    StyleBox["\[CurlyPhi]",
     FontWeight->"Bold"], ")"}], TraditionalForm]],ExpressionUUID->
  "0d503a2a-42b1-4550-b655-183f2c04abcb"],
